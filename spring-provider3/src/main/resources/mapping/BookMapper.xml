<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.intellif.springprovider3.mapper.BookMapper" >
    <insert id="save"  parameterType="cn.intellif.springprovider3.entity.BookEntity">
        INSERT  INTO  yinchong.t_book(name) VALUES (#{name})
    </insert>
    <delete id="delete" parameterType="long">
        DELETE FROM yinchong.t_book where id = #{value}
    </delete>
</mapper>